<%
   init_params = Sample.sound_init_params
   init_params.merge!(Pattern.sound_init_params)
   init_params.merge!(SystemSetting.sound_init_params)
   init_params.merge!({version: PatternStore.version})
%>

<script type="text/javascript">
  var ready_action = function () {
    console.log('initialising')
    Sound.init(
      <%# attributes from rails app %>
      <%= raw init_params.to_json %>
    );
    Console.init();
  }

  $(document).ready(ready_action);
  //$(document).on('page:load', ready_action);
</script>
